{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "jsx": "react",
    "importHelpers": true
  }
}
